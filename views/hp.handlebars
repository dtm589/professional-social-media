{{#if logged_in}}
{{#each users as |user| }}
<div class="container">
    <div class="col text-center">
        <div class="card profile">
            <div class="card-content">
                <h3><a href="/profileapi/profiles/profile/{{this.id}}">{{first_name}} {{last_name}}</a></h3>
            </div>
            <div class="user-title">
                <h4>{{title}}</h4>
            </div>
            <div class="user-img">
                <a href="/profileapi/profiles/profile/{{this.id}}"><img src={{this.img}}></a>
            </div>
        </div>
    </div>
</div>
{{/each}}
{{else}}
<a href='/login'> Log in to view users. </a>
{{/if}}