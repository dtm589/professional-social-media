<section class="container" id="profile-content">
    <div class="col">
    <div id="basic-info">
        <div class="card text-center">
            <!--Should contain pic, name, location, title, company -->
            <div class="card-body">
                <h3 class="card-title">{{userData.first_name}} {{userData.last_name}}</h3>
                <h5>{{userData.title}}</h6>
                <h5>{{userData.company}}</h6>
                <h5>{{userData.location}}</h7>
                <img class="mt-3" src="{{userData.img}}" alt="picture of a user">
            </div>
        </div>
    </div>
    <div id="experience" class="container">
        <!-- Should contain their experience -->
        <div class="card text-center">
            <div class="card-body">
                <h4 class="card-title">Experience</h4>
                <p>{{userData.experience}}</p>
            </div>
        </div>
    </div>
    <div id="education" class="container">
        <!-- Should contain their education -->
        <div class="card text-center">
            <div class="card-body">
                <h4 class="card-title">Education</h4>
                <p>{{userData.education}}</p>
            </div>
        </div>
    </div>
    <div id="skills" class="container">
        <!-- Should contain their skills-->
        <div class="card text-center">
            <div class="card-body">
                <h4 class="card-title">Skills</h4>
                <p>{{userData.skills}}</p>
            </div>
        </div>
    </div>
    </div>
</section>